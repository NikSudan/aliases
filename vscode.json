{
  "git.path": "/usr/local/git",
  "editor.fontFamily": "Fira Code, Menlo, monospace",
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "workbench.editor.showTabs": false,
  "window.restoreFullscreen": true,
  "terminal.external.osxExec": "Hyper.app",
  "workbench.iconTheme": "material-icon-theme",
  "editor.minimap.enabled": true,
  "git.confirmSync": false,
  "files.associations": {
    ".env": "ini",
    ".env.example": "ini",
    ".buckconfig": "ini",
    ".flowconfig": "ini",
    ".gitignore": "ini",
    ".wp-engine-gitignore": "ini",
    ".gitattributes": "ini",
    ".gitmodules": "ini",
    ".eslintignore": "ini",
    ".htaccess": "ini",
    "*.sln": "ini",
    ".prettierrc": "json",
    ".sequelizerc": "javascript",
    ".mdx": "markdown",
    "*.mdx": "markdown",
    "vscode-spotify": "ini",
    "hosts": "ini"
  },
  "files.exclude": {
    ".collabignore": true,
    "**/.ase": true,
    "**/.gitattributes": true,
    "**/.github": true,
    "**/.gitkeep": true,
    "**/.idea": true,
    "**/.nyc_output": true,
    "**/.sass-cache": true,
    "**/.tmp": true,
    "**/.travis.yml": true,
    "**/artisan": true,
    "**/bower_components": true,
    "**/cache": true,
    "**/CHANGELOG.md": true,
    "**/composer.lock": true,
    "**/coverage": true,
    "**/dist": false,
    "**/languages": true,
    "**/LICENSE.md": true,
    "**/license.txt": true,
    "**/node_modules": true,
    "**/phpspec.yml": true,
    "**/phpunit.xml": true,
    "**/readme.html": true,
    "**/server.php": true,
    "**/storage": true,
    "**/upgrade": true,
    "**/uploads": true,
    "**/vendor": true,
    "**/wp-activate.php": true,
    "**/wp-admin": true,
    "**/wp-blog-header.php": true,
    "**/wp-comments-post.php": true,
    "**/wp-cron.php": true,
    "**/wp-includes": true,
    "**/wp-links-opml.php": true,
    "**/wp-load.php": true,
    "**/wp-login.php": true,
    "**/wp-mail.php": true,
    "**/wp-settings.php": true,
    "**/wp-signup.php": true,
    "**/wp-trackback.php": true,
    "**/xmlrpc.php": true,
    "**/yarn.lock": true,
    "android/": true,
    "bootstrap/": true,
    "Builds/": true
  },
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "newUntitledFile",
  "emmet.syntaxProfiles": {
    "php": "html",
    "html": "html",
    "laravel-blade": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "window.zoomLevel": 0,
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.hidesExplorerArrows": false,
  "material-icon-theme.files.associations": {
    "tsconfig.json": "typescript-def",
    "tslint.json": "typescript-def",
    "launch.json": "vscode",
    "settings.json": "vscode"
  },
  "git.autofetch": true,
  "explorer.confirmDragAndDrop": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.arrowParens": "always",
  "prettier.bracketSpacing": true,
  "eslint.packageManager": "yarn",
  "cSpell.language": "en-GB,en",
  "javascript.implicitProjectConfig.checkJs": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "cSpell.showStatus": false,
  "javascript.validate.enable": false,
  "diffEditor.renderSideBySide": true,
  "breadcrumbs.enabled": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdown.extension.toc.githubCompatibility": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.shell.osx": "/bin/zsh",
  "workbench.colorTheme": "One Dark Pro"
}
